<?xml version="1.0" encoding="UTF-8"?>
<Simulation>
  <ConfigInfo>
    <Title>RPSCell</Title>
    <Author>Maverick Chung, mc608</Author>
    <Shape>Rect</Shape>

    <Dimensions>
      <Width>50</Width>
      <Height>50</Height>
      <Speed>.9</Speed>
    </Dimensions>

    <!-- There can be extra parameters, only those relevant will be used. -->
    <SpecialParameters>
      <Parameter name = "RPSThreshhold">2</Parameter>
      <Parameter name = "randomThreshhold">0</Parameter>
    </SpecialParameters>

    <!-- Default is the cells that are filled if there are not enough cells. Color names must be standard HTML Names -->
    <!-- If not specified, DEFAULT is aways the first State (ID: 0)-->
    <States>
      <Default>0</Default>
      <State>
        <ID>0</ID>
        <Color>white</Color>
      </State>

      <State>
        <ID>1</ID>
        <Color>red</Color>
      </State>

      <State>
        <ID>2</ID>
        <Color>green</Color>
      </State>

      <State>
        <ID>3</ID>
        <Color>blue</Color>
      </State>

      <State>
        <ID>4</ID>
        <Color>yellow</Color>
      </State>

      <State>
        <ID>5</ID>
        <Color>pink</Color>
      </State>

    </States>

    <!-- Case insensitive. TRUE means you want to use custom cell locations, below in the <Cells> section. FALSE means you want to have a random grid based on parameters above -->
    <Custom>false</Custom>
  </ConfigInfo>
</Simulation>